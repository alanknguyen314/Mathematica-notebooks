Cell[TextData[{
 "where ",
 Cell[BoxData[
  StyleBox[
   SubscriptBox[
    StyleBox["E",
     FontWeight->"Regular",
     FontSlant->"Italic"], 
    RowBox[{"z", 
     StyleBox[",",
