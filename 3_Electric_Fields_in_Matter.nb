  FontSlant->"Italic"],
 " or \[CapitalDelta]",
 StyleBox["V",
  FontSize->15,
  FontWeight->"Regular",
  FontSlant->"Italic"],
 "  because \[CapitalDelta]",
 StyleBox["V",
  FontSize->15,
  FontWeight->"Regular",
  FontSlant->"Italic"],
 "  is proportional to ",
 StyleBox["Q",
  FontFamily->"Source Sans Pro",
  FontSize->16,
  FontWeight->"Regular",
  FontSlant->"Italic"],
